{
  "name": "ts-udacity-store",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "start": "nodemon src/server.ts",
    "jasmine": "set ENV=test &&jasmine",
    "test": "npm run demigrate-test && npm run migrate-test &&npm run build && set ENV=test&& npm run jasmine",
    "migrate": "db-migrate up",
    "demigrate": "db-migrate reset",
    "migrate-test": "db-migrate --env test up",
    "demigrate-test": "db-migrate --env test reset",
    "create-dev-db": "db-migrate --config database_create.json db:create store",
    "create-test-db": "db-migrate --config database_create.json db:create store_test",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "mohamed elewasy",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.2",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/express-validator": "^3.0.0",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "npm": "^9.3.1",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
